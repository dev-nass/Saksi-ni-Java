

public class Main {
	public static void main (String [] args) {
		String [] [] alphabets = {
									{"a", "b", "c"},
									{"d", "e", "f"},
									{"g", "h", "i"}
								};
		
		// 	[i] rows - [j] columns
		for (int i = 0; i < alphabets.length; i++) {
			for (int j = 0; j < alphabets[i].length; j++) {
				System.out.print(alphabets [i] [j]+ " ");
			}
			System.out.println();
		}
	}
}
