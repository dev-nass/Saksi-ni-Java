import java.util.Random;

public class Main {
	
	public static void main (String [] args) {
		Random random = new Random ();
		
		int x = random.nextInt(6)+1; // random int values.
		/* 6 will change the scope of the random value -
		 * 1 will make sure that the random numbers start with 1
		 */

		double y = random.nextDouble(); // random double value
		boolean z = random.nextBoolean(); // random boolean value
	}
}
