			                try (PreparedStatement studentUpdateStatement = con.prepareStatement(studentUpdateQuery)) {
			                    studentUpdateStatement.setInt(1, guardianIDJava);
			                    
			                    int rowsAffected = studentUpdateStatement.executeUpdate();
			                    if (rowsAffected > 0) {
			                        System.out.println("Update successful. Rows affected: " + rowsAffected);
			                    } else {
			                        System.out.println("No rows updated. No matching FirstName found: ");
			                    }
			                } catch (SQLException w) {
			                    System.err.println("Error executing update statement: " + w.getMessage());
